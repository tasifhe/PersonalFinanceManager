<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="personalfinancemanager.OverviewController">
    <children>
        <VBox alignment="CENTER" spacing="10.0" layoutX="10.0" layoutY="10.0" prefWidth="780.0" prefHeight="580.0">
            <children>
                <Label text="Overview" textFill="BLACK">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Label>
                <Label fx:id="labelBalance" text="Total Balance: $0.00" />
                <Label fx:id="labelExpenses" text="Total Expenses: $0.00" />
                <Label fx:id="labelSavings" text="Total Savings: $0.00" />
                <!-- You can add more details here -->
            </children>
            <padding>
                <Insets top="20.0" bottom="20.0" left="20.0" right="20.0" />
            </padding>
        </VBox>
    </children>
</AnchorPane>
